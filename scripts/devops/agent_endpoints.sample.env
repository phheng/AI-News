# Copy to scripts/devops/agent_endpoints.env and adjust as needed.
# Usage:
#   set -a
#   source scripts/devops/agent_endpoints.env
#   set +a

# Gateway (this workspace)
CRYPTO_INTEL_GATEWAY_BASE=http://127.0.0.1:18080

# External agent services (usually in dedicated workspaces)
CRYPTO_INTEL_MARKET_AGENT_BASE=http://127.0.0.1:18102
CRYPTO_INTEL_STRATEGY_AGENT_BASE=http://127.0.0.1:18103
CRYPTO_INTEL_BACKTEST_AGENT_BASE=http://127.0.0.1:18104

# E2E bootstrap toggles
CRYPTO_INTEL_BOOTSTRAP_RUN_SMOKE=1
CRYPTO_INTEL_BOOTSTRAP_RUN_STRESS=0

# Smoke paths (override only when endpoint paths differ)
CRYPTO_INTEL_STRATEGY_GENERATE_PATH=/v1/strategy/generate
CRYPTO_INTEL_BACKTEST_RUN_PATH=/v1/backtest/run
CRYPTO_INTEL_BACKTEST_STATUS_PATH_TPL=/v1/backtest/runs/{run_id}
CRYPTO_INTEL_STRATEGY_FEEDBACK_PATH=/v1/strategy/feedback/paper-window-closed
CRYPTO_INTEL_NOTIFY_PATH=/v1/notify/telegram/strategy-cycle

# Stress params
CRYPTO_INTEL_MARKET_PATH=/v1/market/ohlcv
CRYPTO_INTEL_STRESS_SYMBOL=BTCUSDT
CRYPTO_INTEL_STRESS_TIMEFRAME=1m
CRYPTO_INTEL_STRESS_BURST=200
CRYPTO_INTEL_STRESS_CONCURRENCY=20
CRYPTO_INTEL_STRESS_TIMEOUT=5
